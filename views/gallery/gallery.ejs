<%- include('../partials/head') -%>
<div class="intro intro-single route bg-image" style="background-image: url(../../images/bride_1.jpg)">
  <div class="overlay-mf"></div>
  <div class="intro-content display-table">
    <div class="table-cell">
      <div class="container">
      <a href="/addGallery">
        <h2 class="intro-title mb-4 pb-4"><span style="color: #f5f5f5">My</span> <span style="color: #ff9f43">Ga</span><span style="color: #f5f5f5">ll</span><span style="color: #ff9f43">er<span><span style="color: #ff9f43">y<span></h2></a>
      </div>
    </div>
  </div>
</div>

<main id="bg">

  
   <section id="work" class="portgallery-mf sect-pt route">
    <div class="container">
      <div class="row">
        <div class="col-sm-12">
          <div class="title-box1 text-center">
            <h3 class="title-a1">
              Pictures
            </h3>
            <p class="subtitle-a">
              Below are Photo shots from the wedding event.
            </p>
            <div class="line-mf"></div>
          </div>
        </div>
      </div>
      <div class="row">
        <% galleries.forEach((gallery)=>{%>
        <div class="col-md-4">
          <div class="work-box">
            <a href="<%= gallery.imgUrl %>" data-gall="Gallery" class="venobox">
              <div class="work-img">
                <img src="<%= gallery.imgUrl %>" alt="" class="img-fluid">
              </div>
            </a>
            <div class="work-content">
              <div class="row">
                <div class="col-sm-8">
                  <h2 class="w-title">View Image</h2>
                  <div class="w-more">
                    <span class="w-date"><%= gallery.createdAt.toLocaleDateString() %></span>
                  </div>
                </div>
                <div class="col-sm-4">
                  <div class="w-like">
                    <a href="/gallery/<%= gallery.id %>"> <span class="ion-ios-plus-outline"></span></a>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <%})%>
      </div>
    </div>
  </section>


  

</main>
<%- include('../partials/footer') -%>